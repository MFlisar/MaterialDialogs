    <!-- ************************ -->
    <!-- Dialog - MAIN PAGE TEXTS -->
    <!-- ************************ -->

    <string name="gdpr_dialog_text1_part1">Dbamy o twoją prywatność i bezpieczeństwo danych.</string>
    <!-- %1$s is replaced with 'gdpr_free'/'gdpr_cheap' string -->
    <string name="gdpr_dialog_text1_part2">Ta aplikacja jest %1$s dzięki używaniu usług firm trzecich.</string>
    <!-- %1$s is replaced with ONE service's name -->
    <string name="gdpr_dialog_text2_singular">Używamy %1$s w tej aplikacji. Ta usługa (i jej partnerzy) zbiera unikalne idenetyfikatorikatory i inne dane personalne. Możesz zmienić swoją decyzję w każdym momencie w ustawieniach aplikacji. &lt;a href=\"\">Dowiedz się więcej o tej usłudze i twoich danych personalnych&lt;/a>.</string>
    <!-- %1$s is replaced with a LIST of service name -->
    <string name="gdpr_dialog_text2_plural">Używamy %1$s w tej aplikacji. Te usługi (i ich partnerzy) zbierają unikalne idenetyfikatorikatory i inne dane personalne. Możesz zmienić swoją decyzję w każdym momencie w ustawieniach aplikacji. &lt;a href=\"\">Dowiedz się więcej o tych usługach i twoich danych personalnych&lt;/a>.</string>
    <string name="gdpr_dialog_text3">Poprzez zgodę potwierdzasz że masz ukończone 16 lat.</string>
    <!-- %1$s is replaced with 'gdpr_dialog_question_ads_info' string if the setup contains an ad network, otherwise with an empty string -->
    <string name="gdpr_dialog_question">Czy możemy kontynuować używania twoich danych dla tych celów%1$s?</string>
    <string name="gdpr_dialog_question_ads_info">\u0020oraz wyświetlania spersonalizowanych reklam</string>

